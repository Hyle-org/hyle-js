{
  "name": "hyle",
  "version": "0.2.2",
  "license": "MIT",
  "repository": "Hyle-org/hyle-js",
  "description": "Hyle js/ts SDK",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc",
    "watch": "./watch.sh",
    "publish": "np --no-tests"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./model": {
      "import": "./dist/model/index.js",
      "types": "./dist/model/index.d.ts"
    }
  },
  "dependencies": {
    "borsher": "^3.6.0"
  },
  "devDependencies": {
    "@types/bun": "^1.1.6",
    "np": "^10.2.0"
  }
}
